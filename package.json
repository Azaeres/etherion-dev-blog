{
  "name": "tailwind-nextjs-starter-blog",
  "version": "1.5.3",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "next dev",
    "build": "contentlayer build && next build",
    "export": "next export",
    "postbuild": "npm run sitemap && npm run rss",
    "sitemap": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-sitemap.mjs",
    "rss": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.0.5",
    "@mailchimp/mailchimp_marketing": "^3.0.58",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "contentlayer": "^0.3.4",
    "esbuild": "^0.18.16",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.2",
    "image-size": "^1.0.0",
    "mdx-bundler": "^9.0.0",
    "next": "^13.4.12",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.5",
    "preact": "^10.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-citation": "^1.0.2",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.1.3",
    "rehype-slug": "^5.0.0",
    "remark": "^14.0.2",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sharp": "^0.32.4",
    "tailwindcss": "^3.0.23",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.12",
    "@svgr/webpack": "^8.0.1",
    "@tailwindcss/forms": "^0.5.4",
    "@types/mailchimp__mailchimp_marketing": "^3.0.10",
    "@types/react": "^18.2.15",
    "@types/tailwindcss": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "cross-env": "^7.0.3",
    "dedent": "^1.2.0",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.12",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^5.0.0",
    "file-loader": "^6.0.0",
    "globby": "^13.2.2",
    "husky": "^8.0.3",
    "inquirer": "^9.2.8",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
